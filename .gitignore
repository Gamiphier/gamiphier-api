# Parameters
/api-command/app/config/parameters.yml
/api-command/app/config/parameters.ini
/api-query/app/config/parameters.yml
/api-query/app/config/parameters.ini

# Managed by Composer
/api-command/app/bootstrap.php.cache
/api-command/var/bootstrap.php.cache
/api-command/bin/*
!api-command/bin/console
!api-command/bin/symfony_requirements
/api-command/vendor/
