!api-command/var/logs/.gitkeep
!api-command/var/sessions/.gitkeep
/api-query/var/cache/*
/api-query/var/logs/*
/api-query/var/sessions/*
!api-query/var/cache/.gitkeep
!api-query/var/logs/.gitkeep
